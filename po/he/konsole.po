#, kde-format
msgctxt "@info:shell"
msgid "Use the internal FALLBACK profile"
msgstr "השתמש בפרופיל FALLBACK המובנה"

#: Application.cpp:66
#, kde-format
msgctxt "@info:shell"
msgid "Set the initial working directory of the new tab or window to 'dir'"
