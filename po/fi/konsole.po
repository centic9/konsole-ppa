#, kde-format
msgctxt "@info:shell"
msgid "Run in a separate process"
msgstr "Suorita erillisenä prosessina"

#: Application.cpp:91
#, kde-format
msgctxt "@info:shell"
msgid "Show the menubar, overriding the default setting"
msgstr "Näytä valikkorivi oletusasetuksesta riippumatta"

#: Application.cpp:94
#, kde-format
msgctxt "@info:shell"
msgid "Hide the menubar, overriding the default setting"
msgstr "Piilota valikkorivi oletusasetuksesta riippumatta"

#: Application.cpp:97
#, kde-format
msgctxt "@info:shell"
msgid "Show the tabbar, overriding the default setting"
msgstr "Näytä välilehtipalkki oletusasetuksesta riippumatta"

#: Application.cpp:100
#, kde-format
msgctxt "@info:shell"
msgid "Hide the tabbar, overriding the default setting"
msgstr "Piilota välilehtipalkki oletusasetuksesta riippumatta"

